<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:gravity="center_horizontal" >
   
    <TextView 		
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Ingestion/overdose /poisoning"
		style="@style/heading"/>

    <ScrollView 
        android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:scrollbars="vertical">
	   
		<LinearLayout 
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"  
			android:gravity="center_horizontal">
			
		    <LinearLayout
			    android:id="@+id/ingestionOverdosePoisoning_infoLayout"
			    android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:gravity="center_horizontal"
			    >
			
			<!-- Geolocation and weather -->
			<fragment 
	        android:id="@+id/fragId"
	        android:name="com.google.android.gms.maps.MapFragment"
	        android:layout_width="500dp"
	        android:layout_height="300dp"
	        />
			
		    <TextView 
		        android:id="@+id/value"
		        android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text=""
				android:visibility="gone"
		        />
		   
		    
		    <TextView
				  android:id="@+id/bluntWeatherInfo"
				  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:text="weather Info"
				  android:visibility="gone"
				         />
		   
				
		    <!--  -->
			<TextView
			    android:id="@+id/ingestionOverdosePoisoning_tv_io"			            
				style="@style/sub_headings"
				android:text="Investigating Officer Information"
				
				android:visibility="visible"
			/>
			<GridLayout
			    android:id="@+id/ingestionOverdosePoisoning_rl1"
				android:layout_width="fill_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="4"
        		android:columnCount="2">
							        
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_io_name"
					style="@style/tv_question"
					android:text="Name"
					android:layout_row="0"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_io_name"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="0"
            		android:layout_column="1"/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_io_surname"
					style="@style/tv_question"
					android:text="Surname"
					android:layout_row="1"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_io_surname"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="1"
            		android:layout_column="1"/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_io_rank"
					style="@style/tv_question"
					android:text="Rank"
					android:layout_row="2"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_io_rank"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="2"
            		android:layout_column="1"/>
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_io_cell"
					style="@style/tv_question"
					android:text="Cell"
					android:layout_row="3"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_io_cell"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="3"
            		android:layout_column="1"/>				       
			</GridLayout>
			
			<TextView
			    android:id="@+id/ingestionOverdosePoisoning_tv_foos"			            
				style="@style/sub_headings"
				android:text="First Officer On Scene"
				
			/>
			<GridLayout 
				android:layout_width="fill_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="3"
        		android:columnCount="2">
							        
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_foos_name"
					style="@style/tv_question"
					android:text="Name"
					android:layout_row="0"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_foos_name"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="0"
            		android:layout_column="1"/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_foos_surname"
					style="@style/tv_question"
					android:text="Surname"
					android:layout_row="1"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_foos_surname"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="1"
            		android:layout_column="1"/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_foos_rank"
					style="@style/tv_question"
					android:text="Rank"
					android:layout_row="2"
            		android:layout_column="0"/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_foos_rank"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="2"
            		android:layout_column="1"/>
								       
			</GridLayout>
			</LinearLayout>
			
		    <LinearLayout
			    android:id="@+id/ingestionOverdosePoisoning_demographicLayout"
			    android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:gravity="center_horizontal"
			    >
			<TextView
			    android:id="@+id/ingestionOverdosePoisoning_tv_victimGender"	            
				
				android:text="Victim Gender"
				style="@style/sub_headings"
				android:layout_marginTop="10dp"
			
			/>
			<GridLayout 
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="1"
        		android:columnCount="1">
			<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgGender"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="left"
			    android:orientation="horizontal"
			    android:layout_row="0"
            	android:layout_column="0"
			    
			    >
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbMale"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Male"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbFemale"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Female"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbUnknownGender"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Unknown"
			    	
			        />
			</RadioGroup>
			</GridLayout>
			
			<TextView
			    android:id="@+id/ingestionOverdosePoisoning_tv_victimRace"			            
				style="@style/sub_headings"
				android:text="Victim Race"
				
				android:layout_marginTop="10dp"
				
				
			/>
			<GridLayout 
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="1"
        		android:columnCount="1">
			<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgRace"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="left"
			    android:orientation="horizontal"
			    android:layout_row="0"
            	android:layout_column="0"
			    >
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbAsian"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Asian"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbBlack"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Black"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbColoured"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Coloured"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbWhite"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="White"
			    	
			        />
			    <RadioButton 
			        android:id="@+id/ingestionOverdosePoisoning_rgbUnknownRace"
			        android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:checked="false"
			    	android:text="Unknown"
			    	
			        />
			</RadioGroup>
			</GridLayout>
			
			<TextView
			    android:id="@+id/ingestionOverdosePoisoning_tv_victimInfo"			            
				style="@style/sub_headings"
				android:text="Victim Information (if known individual)"
				android:layout_margin="5dp"
				
				
			/>
			<GridLayout 
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="9"
        		android:columnCount="2">
							        
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_victim_name"
					android:layout_width="100dp"
					android:layout_height="wrap_content"
					android:text="Victim Name"
					android:layout_row="0"
            		android:layout_column="0"
            		style="@style/tv_question"
					/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_victim_name"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="0"
            		android:layout_column="1"
					/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_victim_surname"
					android:layout_width="100dp"
					android:layout_height="wrap_content"
					style="@style/tv_question"
					android:text="Victim Surname"
					android:layout_row="1"
            		android:layout_column="0"
					/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_victim_surname"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					
					android:layout_row="1"
            		android:layout_column="1"
					/>
							       
							       
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_victim_id"
					android:layout_width="100dp"
					android:layout_height="wrap_content"
					
					android:text="ID Number"
					style="@style/tv_question"
					android:layout_row="2"
            		android:layout_column="0"
					/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_victim_id"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					
					android:layout_row="2"
            		android:layout_column="1"
					/>
				<TextView 
					android:id="@+id/ingestionOverdosePoisoning_tv_victim_age"
					android:layout_width="100dp"
					android:layout_height="wrap_content"
					android:text="Victim Estimated age"
					style="@style/tv_question"
					android:layout_row="3"
            		android:layout_column="0"
					/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_victim_age"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					android:layout_row="3"
            		android:layout_column="1"
					/>
								       
				</GridLayout>
				</LinearLayout>
				
		    <!-- The body Starts  -->
		    	<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_the_body"
					style="@style/main_headings"
					android:layout_marginTop="100dp"
					
					android:text="@string/the_body"
					
					/>
		    	<GridLayout
			    android:id="@+id/ingestionOverdosePoisoning_theBodyLayout"
			    android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:layout_marginLeft="10dp"
				android:paddingLeft="20dp"
				android:rowCount="9"
        		android:columnCount="2"
			    >
				
				<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_bodyDecomposed"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Is the body decomposed?"
					android:layout_row="0"
            		android:layout_column="0"
					
					
					/>
				
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgBodyDecomposed"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    
			    android:layout_row="0"
            	android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_bodyDecomposedYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_bodyDecomposedNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup> 
				
				    
				<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_medicalIntervention"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Is there any medical intervention?"
					
					android:layout_row="1"
            		android:layout_column="0"
					/>
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgMedicalIntervention"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    
			    android:layout_row="1"
            		android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_medicalInterventionYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_medicalInterventionNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup> 
				
				<TextView
				    android:id="@+id/ingestionOverdosePoisoning_tv_whoFoundVictimBody" 
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Who found victim body?"
					
					android:layout_row="2"
            		android:layout_column="0"
					/>
				<EditText
					android:id="@+id/ingestionOverdosePoisoning_whoFoundVictimBody"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					
					android:layout_row="2"
            		android:layout_column="1"
					/>
				
				<TextView
				    android:id="@+id/ingestionOverdosePoisoning_tv_closeToWater"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Was the victim found close to water?"
					
					android:layout_row="3"
            		android:layout_column="0"
					/>
				
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgCloseToWater"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    
			    android:layout_row="3"
            		android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_closeToWaterYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_closeToWaterNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup> 
				 
				<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_suicideSuspected"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Suspicion of suicide?"
					
					android:layout_row="4"
            		android:layout_column="0"
					/>
				
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgSuicideSuspected"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    
			    android:layout_row="4"
            		android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_suicideSuspectedYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_suicideSuspectedNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup>
				
				<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_previousAttempts"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Any previous attempts?"
					
					android:layout_row="5"
            		android:layout_column="0"
					/>
				
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgPreviousAttempts"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    
			    android:layout_row="5"
            		android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_previousAttemptsYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_previousAttemptsNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup>
				<TextView 
				    android:id="@+id/ingestionOverdosePoisoning_tv_howManyAttempts"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="How many attempts?"
				
					android:layout_row="6"
            		android:layout_column="0"
					/>
				<EditText 
					android:id="@+id/ingestionOverdosePoisoning_howManyAttempts"
					android:layout_width="200dp"
					android:layout_height="wrap_content"
					
					android:layout_row="6"
            		android:layout_column="1"
					/>
				
				<TextView
				    android:id="@+id/ingestionOverdosePoisoning_tv_suicideNoteFound"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="5dp"
					
					style="@style/tv_question"
					android:text="Is there a Suicide note on scene?"
					android:layout_row="7"
            	android:layout_column="0"
					/>
			
				<RadioGroup 
			    android:id="@+id/ingestionOverdosePoisoning_rgSuicideNoteFound"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal"
			    android:layout_row="7"
            	android:layout_column="1"
			    >
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_SuicideNoteFoundYes"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="yes"
					
				    />
				    
				    <RadioButton
				    android:id="@+id/ingestionOverdosePoisoning_SuicideNoteFoundNo"
				    android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:checked="false"
			    	android:text="no"
					
				    />
				    
				</RadioGroup>
				</GridLayout>
